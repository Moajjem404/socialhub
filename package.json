{
  "name": "reaction-management-dashboard",
  "version": "1.0.0",
  "description": "A beautiful dashboard for managing reactions, comments, and orders with webhook integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "keywords": ["dashboard", "reactions", "comments", "orders", "webhook", "management"],
  "author": "Your Name",
  "license": "MIT"
}
